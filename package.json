{
  "name": "cli-sandbox",
  "version": "1.0.0",
  "description": "Visual testing for your CLI",
  "type": "module",
  "main": "src/index.js",
  "bin": {
    "sandbox": "src/index.js"
  },
  "scripts": {
    "start": "node --import hot-hook/register src/index.js",
    "dev": "node --watch src/index.js"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "src/",
    "example/"
  ],
  "dependencies": {
    "chalk": "^5.3.0",
    "chokidar": "^3.6.0",
    "clear": "^0.1.0",
    "dependency-tree": "^11.1.1",
    "fuse.js": "^7.0.0",
    "glob": "^10.3.10",
    "hot-esm": "^1.6.0",
    "hot-hook": "^0.4.0",
    "ink": "^6.0.0",
    "neo-blessed": "^0.2.0",
    "react": "^19.1.0"
  },
  "keywords": [
    "cli",
    "testing",
    "errors",
    "terminal",
    "ink",
    "react"
  ],
  "author": "vikfroberg",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vikfroberg/cli-sandbox.git"
  },
  "homepage": "https://github.com/vikfroberg/cli-sandbox#readme",
  "bugs": {
    "url": "https://github.com/vikfroberg/cli-sandbox/issues"
  }
}
